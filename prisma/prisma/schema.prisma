// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model derrames{
  id Int @id @default(0)
  nombre String @unique
  ubicacion String
  paisesi 
  causas
  consecuencias
  soluciones

}